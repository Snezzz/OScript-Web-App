<table class="ui basic selectable table">
<thead>
    <tr>
        <th>Идентификатор</th>
        <th>Имя</th>
        <th>Фамилия</th>
    </tr>
</thead>
<tbody>
    @foreach(var row in Model) {
        <tr>
            <td><a asp-action="edit" asp-route-id="@row.Идентификатор">@row.Идентификатор</a></td>
            <td>@row.Имя</td>
            <td>@row.Фамилия</td>
        </tr>
    }
</tbody>
</table>